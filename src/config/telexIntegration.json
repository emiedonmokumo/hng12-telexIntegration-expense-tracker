{
    "name": "Google Sheets Expense Logger",
    "description": "Logs expenses from Telex messages into a Google Sheets spreadsheet.",
    "integration_type": "output",
    "category": "Finance & Accounting",
    "logo_url": "https://static.sheetgo.com/wp-content/uploads/2020/05/icons-expense-tracker.svg",
    "background_color": "#00454F",
    "settings": {
      "spreadsheet_id": {
        "type": "string",
        "required": true,
        "description": "Google Sheets Spreadsheet ID where expenses will be logged."
      }
    },
    "output": {
      "webhook": {
        "url": "https://hng12-telex-integration-expense-tracker.vercel.app/webhook",
        "method": "POST",
        "headers": {
          "Content-Type": "application/json"
        },
        "payload": {
          "text": "{{text}}",
          "sender": "{{sender}}"
        },
        "response": {
          "success": {
            "status": 200,
            "message": "Expense logged successfully"
          },
          "error": {
            "status": 500,
            "message": "Internal Server Error"
          }
        }
      }
    }
  }
  